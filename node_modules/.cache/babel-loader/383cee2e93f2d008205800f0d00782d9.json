A,EAsBA;AACA,QAAA,KAAA,EAAA,WADA;AAEA,QAAA,IAAA,EAAA,qBAFA;AAGA,QAAA,OAAA,EAAA,mBAAA;AACA,UAAA,KAAA,CAAA,MAAA;AACA;AALA,OAtBA,CAhBA;AA8CA,MAAA,YAAA,EAAA;AA9CA,KAAA;AAgDA,GAnDA;AAoDA,EAAA,eAAA,EAAA,IApDA;AAqDA,EAAA,OArDA,qBAqDA;AACA,SAAA,eAAA,GAAA,IAAA,eAAA,EAAA;AACA,GAvDA;AAwDA,EAAA,OAxDA,qBAwDA;AACA,SAAA,MAAA;AACA,GA1DA;AA2DA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,2BACA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KAHA;AAIA,IAAA,aAJA,2BAIA;AACA,WAAA,QAAA,GAAA,KAAA,gBAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KAPA;AAQA,IAAA,MARA,oBAQA;AAAA;;AACA,WAAA,eAAA,CAAA,MAAA,GAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,IAAA,CAAA,IAAA;AACA,OAFA;AAGA,KAZA;AAaA,IAAA,MAbA,qBAaA;AAAA;;AACA,UAAA,OAAA,CAAA,sCAAA,CAAA,EAAA;AACA,aAAA,eAAA,CAAA,MAAA,CAAA,KAAA,gBAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,YAAA,MAAA,CAAA,MAAA,CAAA,GAAA,CAAA;AAAA,cAAA,QAAA,EAAA,SAAA;AAAA,cAAA,OAAA,EAAA,sBAAA;AAAA,cAAA,MAAA,EAAA,kCAAA;AAAA,cAAA,IAAA,EAAA;AAAA,aAAA;AACA;AACA,SAJA;AAKA;;AACA,WAAA,MAAA;AACA,KAtBA;AAuBA,IAAA,IAvBA,kBAuBA;AAAA;;AACA,WAAA,eAAA,CAAA,IAAA,CAAA,KAAA,QAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,YAAA,GAAA,KAAA;AACA,UAAA,MAAA,CAAA,QAAA,GAAA;AACA,YAAA,MAAA,EAAA,IADA;AAEA,YAAA,QAAA,EAAA,IAFA;AAGA,YAAA,SAAA,EAAA,IAHA;AAIA,YAAA,QAAA,EAAA;AAJA,WAAA;;AAMA,UAAA,MAAA,CAAA,MAAA;AACA;AACA,OAXA;AAYA,KApCA;AAqCA,IAAA,UArCA,wBAqCA;AACA,WAAA,YAAA,GAAA,KAAA;AACA;AAvCA;AA3DA,CAAA","sourcesContent":["<template>\n\n  <div style=\"margin: 0 auto; width: 80%\">\n  <Toast />\n    <Panel header=\"EXAMEN CARLOS ALBERTO OROZCO ALBITER\">\n      <Menubar :model=\"items\" />\n      <p>Porfavor seleccionar una columna para editar/eliminar</p>\n      <br />\n      <DataTable :value=\"empleados\" :paginator=\"true\" :rows=\"10\" selectionMode=\"single\" :selection.sync=\"selectedEmpleado\" dataKey=\"id\">\n      <Column field=\"id\" header=\"ID\"></Column>\n        <Column field=\"nombre\" header=\"Nombre\"></Column>\n        <Column field=\"apellido\" header=\"Apellido\"></Column>\n        <Column field=\"direccion\" header=\"Dirección\"></Column>\n        <Column field=\"telefono\" header=\"Teléfono\"></Column>\n      </DataTable>\n    </Panel>\n    <Dialog header=\"Crear Empleado\" :visible.sync=\"displayModal\" :modal=\"true\">\n      <span class=\"p-float-label\">\n        <InputText id=\"nombre\" type=\"text\" v-model=\"empleado.nombre\" style=\"width: 100%\" />\n        <label for=\"nombre\">Nombre</label>\n      </span>\n      <br />\n      <span class=\"p-float-label\">\n        <InputText id=\"apellido\" type=\"text\" v-model=\"empleado.apellido\" style=\"width: 100%\" />\n        <label for=\"apellido\">Apellido</label>\n      </span>\n      <br />\n      <span class=\"p-float-label\">\n        <InputText id=\"direccion\" type=\"text\" v-model=\"empleado.direccion\" style=\"width: 100%\" />\n        <label for=\"direccion\">Dirección</label>\n      </span>\n      <br />\n      <span class=\"p-float-label\">\n        <InputText id=\"telefono\" type=\"text\" v-model=\"empleado.telefono\" style=\"width: 100%\" />\n        <label for=\"telefono\">Teléfono</label>\n      </span>\n      <template #footer>\n        <Button label=\"Guardar\" icon=\"pi pi-check\" @click=\"save\" />\n        <Button label=\"Cancelar\" icon=\"pi pi-times\" @click=\"closeModal\" class=\"p-button-secondary\" />\n      </template>\n    </Dialog>\n  </div>\n</template>\n\n<script>\nimport empleadoService from \"../service/EmpleadoService\";\nexport default {\n  name: \"CrudApp\",\n  data() {\n    return {\n      empleados: null,\n      empleado: {\n        id:null,\n        nombre: null,\n        apellido: null,\n        direccion: null,\n        telefono: null\n      },\n      selectedEmpleado:{\n        id:null,\n        nombre: null,\n        apellido: null,\n        direccion: null,\n        telefono: null\n      },\n      items: [\n        {\n          label: \"Nuevo\",\n          icon: \"pi pi-fw pi-plus\",\n          command: () => {\n            this.showSaveModal();\n          }\n        },\n        {\n          label: \"Editar\",\n          icon: \"pi pi-fw pi-pencil\",\n          command: () => {\n            this.showEditModal();\n          }\n        },\n        {\n          label: \"Eliminar\",\n          icon: \"pi pi-fw pi-trash\",\n           command: () => {\n            this.delete();\n          }\n        },\n        {\n          label: \"Refrescar\",\n          icon: \"pi pi-fw pi-refresh\",\n          command: () => {\n            this.getAll();\n          }\n        }\n      ],\n      displayModal: false\n    };\n  },\n  empleadoService: null,\n  created() {\n    this.empleadoService = new empleadoService();\n  },\n  mounted() {\n    this.getAll();\n  },\n  methods: {\n    showSaveModal() {\n      this.displayModal = true;\n    },\n     showEditModal() {\n      this.empleado = this.selectedEmpleado;\n       this.displayModal = true;\n    },\n    getAll() {\n      this.empleadoService.getAll().then(data => {\n        this.empleados = data.data;\n      });\n    },\n     delete() {\n     if(confirm(\"Estas seguro de eliminar el registro\")){\n      this.empleadoService.delete(this.selectedEmpleado.id).then(data => {\n       if(data.status===200){\n        this.$toast.add({severity:'success', summary: 'Se elimino con exito', detail:'El registro se elimino con exito', life: 3000});\n       }\n      });\n      }\n      this.getAll();\n    },\n    save() {\n      this.empleadoService.save(this.empleado).then(data => {\n        if (data.status === 200) {\n          this.displayModal = false;\n          this.empleado = {\n            nombre: null,\n            apellido: null,\n            direccion: null,\n            telefono: null\n          };\n          this.getAll();\n        }\n      });\n    },\n    closeModal() {\n      this.displayModal = false;\n    }\n  }\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/components"}]}